{% extends 'drymekPheanstalkBundle::base.html.twig' %}

{% block body %}
    <table>
        <tbody>
            {% for key, value in stats %}
                <tr>
                    <td>
                        {{ key }}
                    </td>
                    <td>
                        {{ value }}
                    </td>
                </tr>
            {% endfor %}
        </tbody>
        <thead>
            <tr>
                <th>Variable</th>
                <th>Value</th>
            </tr>
        </thead>
    </table>
{% endblock %}
